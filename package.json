{
  "name": "desafio-xp",
  "version": "1.0.0",
  "description": "Repositório destinado ao versionamento do código da aplicação do desafio de back-end da turma XP.",
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node src/server.ts' -e ts",
    "build": "tsc",
    "test": "node_modules/.bin/jest --watchAll --runInBand --verbose",
    "test2": "tsc --project tsconfig.json && jest --rootDir=dist --runInBand --detectOpenHandles",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/typeorm-config.ts"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.3.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.10",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/express": "4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "^16.11.45",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "better-sqlite3": "^7.6.2",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "nodemon": "^1.19.4",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlanFernandes86/desafio-xp.git"
  },
